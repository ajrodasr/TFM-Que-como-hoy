<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.uoc.tfm.qch.security.service.RolRepository">

	<select id="getRolByNombre" resultType="com.uoc.tfm.qch.security.domain.Rol">
		SELECT *
		FROM ROLES
		WHERE nombre = #{rolNombre}
	</select>

</mapper>